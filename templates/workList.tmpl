{{ header "Works" }}
            <h1>Works</h1>
            <ul>
                {{ range . }}
                <li>
                    <a href="{{ domain }}/works/{{ .Slug }}{{ index }}">
                    {{ if .Year }}{{ .Year }} - {{ end }}{{ if .AllAuthors }}{{ .AllAuthors }} - {{ else if .Dubious }}Anonymous - {{ end }}{{ .TitleMain }}
                    </a>
                </li>
                {{ end }}
            </ul>
{{ footer }}
