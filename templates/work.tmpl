{{ header .TitleMain }}
            <h1>{{ .TitleMain }}</h1>
            <dl>
                {{ if not .Year }}{{ else }}
                <dt>Created:</dt>
                <dd>{{ .Year }}</dd>
                {{ end }}
                {{ if not .Wikidata }}{{ else }}
                <dt>Wikidata:</dt>
                <dd><a href="https://wikidata.org/wiki/{{ .Wikidata }}">{{ .Wikidata }}</a></dd>
                {{ end }}
                {{ if and (not .WikiName) (not .WikiLang) }}{{ else }}
                <dt>Wikipedia:</dt>
                <dd><a href="https://{{ .WikiLang }}.wikipedia.org/wiki/{{ .WikiName }}">{{ .WikiLang }}.wikipedia.org/wiki/{{ .WikiName }}</a></dd>
                {{ end }}
            </dl>
{{ footer }}
